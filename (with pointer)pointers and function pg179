//using pointer
#include "pch.h"
#include <iostream>
using namespace std;
void cubeByReference(int *);
int main()
{
	int number = 5;
	cout << "The original value of the number is " << number << endl;
	cubeByReference(&number);
	cout << "\nThe new value of number is " << number << endl;
}
void cubeByReference(int *nPtr)
{
	*nPtr = *nPtr**nPtr**nPtr;
}
